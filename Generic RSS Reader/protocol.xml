		<QAction id="10" name="Read RSS" encoding="csharp" triggers="10">
		</QAction>
	</QActions>

	<Groups>
		<Group id="2">
			<Name>After Startup</Name>
			<Description>After Startup</Description>
			<Type>poll action</Type>
			<Content>
				<Action>2</Action>
			</Content>
		</Group>
		<Group id="10">
			<Name>Read RSS</Name>
			<Description>Read RSS</Description>
			<Type>poll action</Type>
			<Content>
				<Action>10</Action>
			</Content>
		</Group>
	</Groups>

	<Triggers>
		<Trigger id="1">
			<Name>After Startup</Name>
			<On>protocol</On>
			<Time>after startup</Time>
			<Type>action</Type>
			<Content>
				<Id>1</Id>
			</Content>
		</Trigger>
	</Triggers>

	<Actions>
		<Action id="1">
			<Name>After Startup Group</Name>
			<On id="2">group</On>
			<Type>execute next</Type>
		</Action>
		<Action id="2">
			<Name>After Startup QAction</Name>
			<On id="2">parameter</On>
			<Type>run actions</Type>
		</Action>
		<Action id="10">
			<Name>Read RSS</Name>
			<On id="10">parameter</On>
			<Type>run actions</Type>
		</Action>
	</Actions>

